{
  "name": "brycleansyourcode",
  "displayName": "BryCleansYourCode",
  "description": "Remove console.log, comments, and debugger statements for cleaner code.",
  "version": "0.0.2",
  "publisher": "bry",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:brycleansyourcode.cleaning"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/enmabry/brycleansyourcode.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "brycleansyourcode.cleaning",
        "title": "Bry: Cleans Your Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enmabry/brycleansyourcode/issues"
  },
  "homepage": "https://github.com/enmabry/brycleansyourcode#readme",
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5"
  }
}
